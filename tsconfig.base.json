{
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "outDir": "dist",
    "module": "commonjs",
    "types": []
  },
  "include": [
    "src"
  ]
}